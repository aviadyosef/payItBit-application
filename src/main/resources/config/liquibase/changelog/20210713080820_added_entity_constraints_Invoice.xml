<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">
    <!--
        Added the constraints for entity Invoice.
    -->
    <changeSet id="20210713080820-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="invoice_info_id"
                                 baseTableName="invoice"
                                 constraintName="fk_invoice__invoice_info_id"
                                 referencedColumnNames="id"
                                 referencedTableName="invoice_info"/>

        <addForeignKeyConstraint baseColumnNames="supplier_info_id"
                                 baseTableName="invoice"
                                 constraintName="fk_invoice__supplier_info_id"
                                 referencedColumnNames="id"
                                 referencedTableName="supplier_info"/>

        <addForeignKeyConstraint baseColumnNames="bank_info_id"
                                 baseTableName="invoice"
                                 constraintName="fk_invoice__bank_info_id"
                                 referencedColumnNames="id"
                                 referencedTableName="bank_info"/>

        <addForeignKeyConstraint baseColumnNames="invoice_id"
                                 baseTableName="invoice"
                                 constraintName="fk_invoice__invoice_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_info"/>
    </changeSet>
</databaseChangeLog>
